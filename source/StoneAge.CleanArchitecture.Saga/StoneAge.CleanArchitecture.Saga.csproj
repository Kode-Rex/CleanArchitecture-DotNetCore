<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Product>StoneAge.CleanArchitecture.Saga</Product>
    <Company>StoneAge Technologies</Company>
    <Authors>StoneAge Technologies</Authors>
    <Description>A set of packages to allow for better usecase compoisition when dealing with large pieces of logic</Description>
    <PackageProjectUrl>https://github.com/StoneAgeTechnologies/CleanArchitecture-DotNetCore</PackageProjectUrl>
    <PackageTags>DotNetCore CleanArchitecture</PackageTags>
    <PackageReleaseNotes>Improved error logging to add inner exception along with wrapping compenstate action in try-catch to avoid it causing problems</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.8</Version>
    <AssemblyVersion>1.0.8.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StoneAge.CleanArchitecture.Domain.DotNetCore" Version="4.0.6" />
  </ItemGroup>

</Project>

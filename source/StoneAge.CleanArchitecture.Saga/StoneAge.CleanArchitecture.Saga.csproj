<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Product>StoneAge.CleanArchitecture.Saga</Product>
    <Company>StoneAge Technologies</Company>
    <Authors>StoneAge Technologies</Authors>
    <Description>A set of packages to allow for better usecase compoisition when dealing with large pieces of logic</Description>
    <PackageProjectUrl>https://github.com/StoneAgeTechnologies/CleanArchitecture-DotNetCore</PackageProjectUrl>
    <PackageTags>DotNetCore CleanArchitecture</PackageTags>
    <PackageReleaseNotes>Added error behavior on error so you can continue to halt execution</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StoneAge.CleanArchitecture.Domain.DotNetCore" Version="4.0.2" />
  </ItemGroup>

</Project>

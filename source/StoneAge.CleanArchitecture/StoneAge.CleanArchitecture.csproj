<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Travis Frisinger</Authors>
    <Company>StoneAge Technologies</Company>
    <Description>A set of .net utilities to make it simple to implement the clean architecture. Mainly file, error and JSON object presenters.</Description>
    <AssemblyName>StoneAge.CleanArchitecture</AssemblyName>
    <RootNamespace>StoneAge.CleanArchitecture</RootNamespace>
    <PackageId>StoneAge.CleanArchitecture.DotNetCore</PackageId>
    <Version>3.7.6</Version>
    <AssemblyVersion>3.7.6.0</AssemblyVersion>
    <FileVersion>3.7.6.0</FileVersion>
    <Product>StoneAge.CleanArchitecture.DotNetCore</Product>
    <PackageProjectUrl>https://github.com/StoneAgeTechnologies/CleanArchitecture-DotNetCore</PackageProjectUrl>
    <PackageTags>CleanArchitecture DotNetCore</PackageTags>
    <PackageReleaseNotes>Fixed up issue with SuccessOrUnauthorizedRestfulPresenter not allowing status code render to be called</PackageReleaseNotes>
    <PackageIconUrl>http://www.horn-it.com/wp-content/uploads/2016/11/Layers.jpg</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.1.4" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.3" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="StoneAge.CleanArchitecture.Domain.DotNetCore" Version="3.7.4" />
  </ItemGroup>

</Project>